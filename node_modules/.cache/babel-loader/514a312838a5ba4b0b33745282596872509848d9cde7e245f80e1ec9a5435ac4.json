e) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.loading = true;\r\n          setTimeout(() => {\r\n            this.loading = false;\r\n          }, 3000);\r\n        } else {\r\n          console.log(\"error submit!!\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.login_body{\r\n  width: 1490px;\r\n  height: 730px;\r\n  background-color: #bb22ae;\r\n  background-image: url(\"../static/picture/0001.png\");\r\n}\r\n\r\n.main_form{\r\n  position: absolute;\r\n  width: 650px;\r\n  margin-left: 650px;\r\n  margin-top: 200px;\r\n  background-color: wheat;\r\n  height: 300px;\r\n}\r\n.demo-ruleForm{\r\n  padding-top: -30px;\r\n  padding-left: 0px;\r\n  width: 600px;\r\n\r\n}\r\n\r\n#btn{\r\n  position: absolute;\r\n  padding-left: 150px;\r\n}\r\n.login_foot{\r\n  height: 60px;\r\n  background-color: #407dcc;\r\n  position: absolute;\r\n  padding-top: 200px;\r\n  font-size: smaller;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}